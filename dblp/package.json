{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dblp",
  "title": "DBLP Search",
  "description": "Search the DBLP database for publications",
  "icon": "dblp-icon.png",
  "author": "koayon",
  "contributors": [
    "pernielsentikaer",
    "NickyMouse"
  ],
  "categories": [
    "Productivity",
    "Web",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search DBLP",
      "description": "Search DBLP for publications",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.52.1",
    "@raycast/utils": "^1.4.8",
    "date-fns": "^2.23.0",
    "natural": "^6.3.0",
    "npm": "^9.6.7",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/xml2js": "^0.4.11",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3",
    "node-fetch": "^3.0.0"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
